@article{Gamble1997,
	Abstract = {Dans la lit\'erature il n'est pas tr\`es claire si la possibilit\'e
	de l\'egislation directe ``substitutive'' (le parlementne peut pas
	l'emp\^echer) a un effet positive ou n\'egative sur les droits des
	minorit\'es. En collectionant des donn\'ees sur des initiatives et
	r\'ef\'erendums dans les \'etats am\'ericains et des localit\'es,
	l'auteur essaie de voir si les droits des minorit\'es souffrent dans
	l'ar\`ene populaire. Ce qu'elle trouve c'est que dans les domaines
	de ``housing'', ``schoold desegregation,'' ``Gay rights,'' ``English
	language laws'' les droits des minorit\'es ont \'et\'e d\'efaite
	en grande majorit\'e dans le processus r\'ef\'erendaire. Souvent
	des nouvelles lois adopt\'ees par le parlement ont \'et\'e abolies
	ou remplac\'ees dans ce processus. Seulement dans le domaine de l'``Aids''
	ce effet n\'egative ne se fait pas sentir. Tout en admettant qu'il
	faudrait comparer ces r\'esultats avec ceux de la d\'emocratie repr\'esentative,
	l'auteur conclue en disant que ``the record shows that American voters
	readily repeal existing civil rights protections and enthousiastically
	enact laws that bar their elected representatives from passing new
	ones.'' (262) (r\'eplique par Donovan and Bowler, 1998 et Frey et
	Goette 1998).},
	Annote = {moi},
	Author = {Gamble, Barbara},
	Date-Added = {2009-12-15 09:12:45 -0500},
	Date-Modified = {2009-12-15 09:12:45 -0500},
	Journal = {American Journal of Political Science},
	Keywords = {kings ucr mrdd ref usa},
	Number = {1},
	Pages = {245-269},
	Title = {Putting Civil Rights to a Popular Vote},
	Volume = {41},
	Year = {1997}}

